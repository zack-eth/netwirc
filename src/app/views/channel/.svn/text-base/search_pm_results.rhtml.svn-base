<% if @results.empty? %>
  '<%=h @phrase %>' not found!
<% else %>
<% unless @phrase.length == 0 %>
  '<%=h @phrase %>' found in
  <% for result in @results %>
  	<br><%= result.name %><%= link_to("pm", :controller => "channel", :action => "pm_invite", :user_id => result.id, :channel_id => @channel_id) %>
  <% end %>
<% end %>
<% end %>