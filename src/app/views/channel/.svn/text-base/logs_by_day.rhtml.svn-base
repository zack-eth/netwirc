<div class="panel">
	<div class="panel_header"><%= render(:partial => 'main/panel_header', :locals => {:title => "View Logs", :current => @channel.name}) %></div>
<table style="position: absolute; left: 25px">
<% for log in @date_query %>
	<tr><td>
	<%= link_to (log.date, :controller => "channel", :action => "logs", :id =>  @channel.id, :date => log.date) %>
	</td></tr>
<% end %>
</table>
</div>