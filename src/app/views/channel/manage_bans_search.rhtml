<% if @results.empty? %>
  '<%=h @phrase %>' not found!
<% else %>
<% unless @phrase.length == 0 %>
  '<%=h @phrase %>' found in
  <% for result in @results %>
  	<br><%= link_to(result.name, { :controller => "user", :action => "profile", :id => result.id }) %> | <%= link_to_remote("ban",
									:update => "current_bans",
									:url => { :controller => "channel", :action => "manage_bans_add", :channel_id => @channel_id, :banned_id => result.id }) %>
  <% end %>
<% end %>
<% end %>