<div class="panel">
	<div class="panel_header">
		<%= render(:partial => 'main/panel_header', :locals => {:title => "Manage Bans", :current => @channel.name}) %>
	</div>
<div id="manage_ops">
<p>Current Bans:</p>
<div id="current_bans">
	<%= render(:partial => "channel/manage_bans_ban", :collection => @bans) %>
</div>
<p><%= text_field_tag :searchQuery %></p>
<div id="searchResults"></div>
<%= observe_field(:searchQuery,
					:frequency => 0.5,
					:update => 'searchResults',
					:url => { :controller => 'channel', :action => 'manage_bans_search', :channel_id => @channel.id }) %>
</div>
</div>