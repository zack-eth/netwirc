<div class="panel">
<table width='100%'>
	<tr>
		<td><u>Name:</u></td>
		<td><u>Topic:</u></td>
	</tr>
<% for r in @results %>
	<tr class="<%= cycle('odd', 'even') -%>">
	<td><%= link_to (r.name, { :controller => "channel", :action => "visit", :id => r.id }, :style => "background-color: transparent;") %></span></td>
	<td><%= r.topic %></td>
	</tr>
<% end %>
</table>
</div>