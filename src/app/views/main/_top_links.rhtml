<div id="top_links">
	<% if logged_in? %>
		<%= link_to("home", :controller => "main", :action => "select_main_tab", :c => "user", :a => "home") %> |
		<%= link_to("logout", :controller => "main", :action => "select_main_tab", :c => "user", :a => "logout") %> |
	<% else -%>
		<% 6.times do %>
		&nbsp;
		<% end %>
		<%= link_to("home", :controller => "main", :action => "select_main_tab", :c => "main", :a => "index") %> | 
  	<% end -%>
	<%= link_to("help", :controller => "main", :action => "select_main_tab", :c => "main", :a => "help") %>
</div>